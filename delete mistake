      "source": [
        "from sentence_transformers import SentenceTransformer\n",
        "import numpy as np\n",
        "import faiss\n",
        "import json\n",
        "from transformers import pipeline"
      ],
      "metadata": {
        "id": "NkL1D8KhdYcG"
      },
      "id": "NkL1D8KhdYcG",
      "execution_count": 24,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 1) Build FAISS index with cosine behavior\n",
        "def build_faiss_index(chunks_path):\n",
        "    # Load model and chunks\n",
        "    model = SentenceTransformer(\"sentence-transformers/all-MiniLM-L6-v2\")\n",
        "    chunks = []\n",
        "    with open(chunks_path, \"r\", encoding=\"utf-8\") as f:\n",
        "        for line in f:\n",
        "            chunks.append(json.loads(line))\n",
        "\n",
        "    # Collect embeddings and normalize to unit length\n",
        "    embeddings = np.array([chunk[\"embedding\"] for chunk in chunks], dtype=\"float32\")\n",
        "    faiss.normalize_L2(embeddings)  # in-place normalization to unit norm\n",
        "\n",
        "    # Use inner product index (dot product == cosine on unit vectors)\n",
        "    d = embeddings.shape[1]\n",
        "    index = faiss.IndexFlatIP(d)\n",
        "    index.add(embeddings)\n",
        "    return model, index, chunks"
      ],
      "metadata": {
        "id": "T8261LDaeUjl"
      },
      "id": "T8261LDaeUjl",
      "execution_count": 26,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# 2) Retrieval with normalized query\n",
        "def retrieve_chunks(query, model, index, chunks, top_k=5):\n",
        "    q = model.encode(query).astype(\"float32\")\n",
        "    faiss.normalize_L2(q.reshape(1, -1))  # normalize query\n",
        "    D, I = index.search(q.reshape(1, -1), top_k)\n",
        "    results = [chunks[i] for i in I[0]]\n",
        "    return results\n"
      ],
      "metadata": {
        "id": "E_ZVWgW9f-2r"
      },
      "id": "E_ZVWgW9f-2r",
      "execution_count": 27,
      "outputs": []
    },
    {
      "cell_type": "code",
